{
  "name": "vue-dashboard",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "postbuild": "rm dist/mockServiceWorker.js",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "test:unit": "vitest",
    "test:story": "test-storybook",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.ts --ignore-path .gitignore",
    "format": "prettier --write src/",
    "gen:schema": "openapi-typescript -t ./openapi.yaml -o ./src/shared/api/v1.schema.ts",
    "postgen:schema": "prettier --write ./src/shared/api/v1.schema.ts",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{vue,ts}": "pnpm format"
  },
  "dependencies": {
    "@vee-validate/zod": "^4.15.0",
    "@vueform/multiselect": "^2.6.11",
    "@vuepic/vue-datepicker": "^8.8.1",
    "date-fns": "^3.6.0",
    "neverthrow": "^6.2.2",
    "openapi-fetch": "^0.9.8",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.9.0",
    "@playwright/test": "^1.49.1",
    "@redocly/cli": "^1.27.0",
    "@rushstack/eslint-patch": "^1.10.4",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@storybook/test-runner": "^0.18.2",
    "@storybook/vue3": "^8.4.7",
    "@storybook/vue3-vite": "^8.4.7",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/vue": "^8.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.17.11",
    "@typespec/compiler": "^0.56.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.1",
    "eslint-plugin-neverthrow": "^1.1.4",
    "eslint-plugin-no-relative-import-paths": "^1.5.5",
    "eslint-plugin-prefer-arrow-functions": "^3.4.1",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unicorn": "^53.0.0",
    "eslint-plugin-vue": "^9.32.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "happy-dom": "^14.12.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "msw": "^2.7.0",
    "msw-storybook-addon": "^2.0.4",
    "npm-run-all2": "^6.2.6",
    "openapi-msw": "^0.7.1",
    "openapi-typescript": "^7.5.1",
    "playwright": "^1.49.1",
    "prettier": "^3.4.2",
    "sass": "^1.83.1",
    "storybook": "^8.4.7",
    "typescript": "~5.4.5",
    "vite": "^5.4.11",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.2.0"
  },
  "volta": {
    "node": "22.12.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}